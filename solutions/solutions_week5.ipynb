   "outputs": [],
   "source": [
    "def area(a, b=None):\n",
    "    \"\"\"Compute the area of a rectangle. If the second argument is not provided, a square is assumed.\"\"\"\n",
    "    if b:\n",
    "        c = b\n",
    "    else:\n",
    "        c = a\n",
    "    return a * c"
   ]
